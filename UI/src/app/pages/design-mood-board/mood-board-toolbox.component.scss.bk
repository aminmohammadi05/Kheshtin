// Select Category
#pattern-menu.menu {
    position: fixed;
    // top: 8%;
    top: 6.625rem;
    right: 8%;
    // bottom: 8%;
    left: calc(0% + 350px);
    max-width: 45vw;
    background-color: $app-panel-bg;
    border-radius: $app-panel-radius;
    box-shadow: 0 0 20px rgb(0 0 0 / 25%);
    display: none;
    z-index: 900;
}

