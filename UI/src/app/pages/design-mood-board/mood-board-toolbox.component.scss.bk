.diamond {
    position: fixed;
    // left: calc(8% + 330px);
    right: calc(27%);
    top: 214px;
    z-index: -25;
    height: 46px;
    -webkit-filter: drop-shadow(0px 0px 15px rgba(0, 0, 0, 0.25));
    filter: drop-shadow(0px 0px 15px rgba(0, 0, 0, 0.25));
}

.diamond polygon {
    fill: $app-panel-bg;
}

.header {
    border-bottom: $app-section-border;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 $app-section-margin;
}

.menu-title {
    display: inline-block;
    color: $app-section-title-color;
    font-weight: $app-section-title-weight;
    // font-size: $app-section-title-color-size;
}

.icon {
    width: 20px;
    float: right;
    opacity: .2;
}

.menu-content {
    overflow-y: scroll;
    // max-height: calc(84vh - 51px);
    max-height: calc(74vh - 50px);
    text-align: left;
    box-sizing: border-box;
    background-color: $app-panel-bg;
    border-radius: 0 0 $app-panel-radius $app-panel-radius;
}


.sidebar-section {
    padding: $app-section-margin;
    border-top: $app-section-border;
    padding-bottom: 20px;
}

.sidebar-section:first-child {
    border-top: none;
}

@media only screen and (min-width: 1281px) {
    .menu-option {
        width: 14%;
        margin: 3%;
        margin-bottom: 35px;
    }
}

.menu-option {
    position: relative;
    display: inline-block;
    float: left;
    width: 14%;
    margin: 3%;
    margin-top: 35px;
    margin-bottom: 35px;
    font-size: 12px;
}

.option-img-container.pattern-img {
    background-size: 40%;
}

.option-img-container {
    width: 100%;
    padding-bottom: 100%;
    position: relative;
    overflow: hidden;
    border-radius: 50%;
    box-sizing: border-box;
    background-color: #eee;
    background-size: cover;
}

.menu-option-title {
    margin-top: 25px;
    overflow: hidden;
    text-overflow: ellipsis;
    font-weight: 300;
    text-align: center;
}

