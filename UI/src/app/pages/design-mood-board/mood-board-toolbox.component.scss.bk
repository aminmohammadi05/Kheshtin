$font-family: 'Vazir FD', 'sans-serif';
$app-panel-bg: #ffffff;
$main-accent-color: #4c72ff;
$main-accent-color-darken: #5973d0;
$app-panel-border: 1px solid red;
$app-panel-radius: 12px;
$app-accent-color: #4c72ff;
$app-font-color: #464646;
$app-icon-color: black;
$app-input-bg: #eee;
$app-input-bg-hover: #e1e1e1;
$app-input-color: #464646;
$app-input-radius: 7px;
$app-input-border: none;
$app-input-font-size: 18px;
$app-input-height: 45px;
$app-label-color: #555;
$app-label-font-size: 11px;
$app-title-color: #464646;
$app-title-font-size: 15px;
$app-title-weight: 500;
$app-section-title-color: #464646;
$app-section-title-font-size: 15px;
$app-section-title-weight: 500;
$app-section-margin: 15px;
$app-section-border: 1px solid #eee;
$app-slider-color: #777;
$app-copyright-color: #666;
$app-tab-border: 1px solid #999;
$app-tab-color: #777;
$app-tab-active-bg: #999;
$app-tab-active-color: #FFF;
$app-scrollbar-color: #ddd;
$app-button-height: 46px;
$app-button-radius: 5px;
$app-button-color: $app-accent-color;
$app-button-color-active: white;
$app-button-font-size: 18px;
$app-button-bg: rgba(0, 0, 0, 0);
$app-button-border: 2px solid $app-accent-color;
$app-button-box-shadow: none;

#controls {
    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    // left: 8%;
    right: 0;
    // top: 8%;
    // bottom: 8%;
    // width: 302px;
    width: 100%;
    background-color: $app-panel-bg;
    border-radius: $app-panel-radius;
    box-shadow: 0 0 20px rgba(0, 0, 0, .25);
    user-select: none;
}

.header {

    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    border-bottom: $app-section-border;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 $app-section-margin;
}

.header>a {
    text-decoration: none;
    color: $main-accent-color;
}

#logo {

    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    user-select: none;
    color: $main-accent-color;
    width: 20px;
    height: 20px;
    display: block;
    float: left;
}

#_45px {

    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    user-select: none;
    color: $main-accent-color;
    width: 100%;
    height: auto;
}

div#kebab {


    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    height: 20px;
    width: 20px;
    float: right;
    opacity: .2;
}

svg#kebab {


    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    width: 100%;
    height: auto;
}

#download-settings {

    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    padding: $app-section-margin;
    border-top: $app-section-border;
    padding-bottom: 20px;
    font-size: 11px;
}

#download {

    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    font-size: 11px;
}

.button2 {
    font-family: $font-family;
    background-color: $app-button-bg;
    border: $app-button-border;
    color: $app-accent-color;
    border-radius: $app-button-radius;
}

button,
input[type=submit] {
    color: $app-button-color-active;
    background-color: $app-accent-color;
    border: none;
    border-radius: $app-button-radius;
    font-size: $app-button-font-size;
    width: 100%;
    max-width: 400px;
    height: $app-button-height;
}

.button2:hover,
.button-link:hover {
    cursor: pointer;
    background-color: $app-accent-color;
    color: $app-button-color-active;
    text-decoration: none;
    box-shadow: none;
}

button:hover,
input[type=submit]:hover {
    cursor: pointer;
    text-decoration: none;
    box-shadow: inset 0 0 100px 5px rgb(0 0 0 / 20%);
}

.button2:hover,
.button-link:hover {
    cursor: pointer;
    background-color: $app-accent-color;
    color: $app-button-color-active;
    text-decoration: none;
    box-shadow: none;
}

.button2:active,
.button-link:active {
    color: $app-button-color-active;
    background-color: $app-accent-color;
    outline: none;
    box-shadow: inset 0 0 15px 5px rgb(0 0 0 / 30%);
}

.scrollable-area {
    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    overflow-y: scroll;
    height: calc(100% - 140px);
}

#texture-controls {
    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
}

#pattern-section {
    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    padding: $app-section-margin;
    padding-bottom: 20px;
    border-top: none;
}

.section-header {
    -webkit-font-smoothing: antialiased;
    user-select: none;
    font-weight: $app-section-title-weight;
    font-size: $app-section-title-font-size;
    color: $app-section-title-color;
    margin-bottom: 10px;
}

.input-row {
    font-size: 1em;
    -webkit-font-smoothing: antialiased;
    color: $app-font-color;
    user-select: none;
    display: flex;
    justify-content: space-between;
}
.input-block {
    width: 100%;
    margin-top: 10px;
}

.pseudo-select {
    font-size: $app-input-font-size;
    border-radius: $app-input-radius;
    background-color: $app-input-bg;
    width: 100%;
    height: $app-input-height;
    display: inline-flex;
    align-items: center;
    justify-content: space-between;

    &:hover {
        background-color: $app-input-bg-hover;
        cursor: pointer;
    }

    .pseudo-select-text {
        margin-right: 12px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        width: calc(100% - 60px);
    }

    &.psuedo-with-rotate {
        // width: calc(100% - $app-input-height - 10px);
        width: calc(100% - 55px);
    }
}



.menu-toggle:hover {
    cursor: pointer;
}


.circle-icon {
    background-position: center;
    background-repeat: no-repeat;
    background-size: 18px 18px;
}

.circle-thumb {
    width: 30px;
    height: 30px;
    display: inline-block;
    border-radius: 50%;
    position: relative;
    float: right;
    top: 0;
    right: 0;
    margin-left: 7px;
    background-size: 60%;
    background-color: #eee;
}

input[type=number],
input[type=text],
input[type=email],
input[type=password],
textarea {
    background-color: $app-input-bg;
    border: $app-input-border;
    border-radius: $app-input-radius;
    width: calc(100% - 20px);
    max-width: 380px;
    font-size: $app-input-font-size;
    height: $app-input-height;
    padding: 0 10px;
    color: $app-input-color;
}

.hidden-input {
    display: none;
}

.app-icon-button {
    // font-size: $app-input-size;
    border: $app-input-border;
    background-color: $app-input-bg;
    border-radius: $app-input-radius;
    width: $app-input-height;
    height: $app-input-height;
    display: inline-flex;
    align-items: center;
    justify-content: center;
}

.app-icon-button:hover {
    cursor: pointer;
    background-color: $app-input-bg-hover;
}

.app-icon {
    height: 20px;
    width: 20px;
}

.svg-icon svg {
    width: 100%;
    height: auto;
}

.input-row {
    display: flex;
    justify-content: space-between;
}

.input-group {
    display: inline-block;
    margin-top: 10px;
    width: calc(50% - 5px);
}

.input-label {
    font-size: $app-label-font-size;
    padding-bottom: 4px;
    color: $app-label-color;
}

input:focus,
input:hover,
select:focus,
select:hover,
textarea:focus {
    background-color: $app-input-bg-hover;
    outline: none;
}

.input-line {
    margin: 15px 0;
}

input[type=checkbox] {
    margin-left: 10px;
}

label {
    margin-right: 5px;
    margin-top: 2px;
}

